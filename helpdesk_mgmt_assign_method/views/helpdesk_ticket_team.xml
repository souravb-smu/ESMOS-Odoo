<odoo>
    <record id="helpdesk_ticket_team_form_view" model="ir.ui.view">
        <field name="name">helpdesk.ticket.team.form (assign method)</field>
        <field name="model">helpdesk.ticket.team</field>
        <field name="inherit_id" ref="helpdesk_mgmt.view_helpdesk_team_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='alias_contact']" position="after">
                <field name="assign_method" />
            </xpath>
        </field>
    </record>
</odoo>
